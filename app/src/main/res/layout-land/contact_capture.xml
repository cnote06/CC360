<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/photoIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_camera_alt_black_18px"
        tools:layout_editor_absoluteY="133dp"
        tools:layout_editor_absoluteX="528dp" />

    <ImageButton
        android:id="@+id/faceBookIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_facebook_24"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="21dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintVertical_bias="0.396"
        app:layout_constraintRight_toLeftOf="@+id/linkedInIcon"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp" />

    <ImageButton
        android:id="@+id/linkedInIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_linkedin"
        app:layout_constraintLeft_toRightOf="@+id/faceBookIcon"
        android:layout_marginLeft="31dp"
        app:layout_constraintTop_toTopOf="@+id/faceBookIcon"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/faceBookIcon"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintRight_toLeftOf="@+id/instagramIcon"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp" />

    <ImageButton
        android:id="@+id/instagramIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_instagram"
        app:layout_constraintLeft_toRightOf="@+id/linkedInIcon"
        android:layout_marginLeft="33dp"
        app:layout_constraintTop_toTopOf="@+id/linkedInIcon"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/linkedInIcon"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintRight_toLeftOf="@+id/SnapIcon"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp" />

    <ImageButton
        android:id="@+id/SnapIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_snapchat"
        app:layout_constraintLeft_toRightOf="@+id/instagramIcon"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintTop_toTopOf="@+id/instagramIcon"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/instagramIcon"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/fullName"
        android:layout_width="233dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Full Name"
        android:inputType="textPersonName"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/mobilePhoneNumber"
        android:layout_width="223dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Mobile Phone Number"
        android:inputType="phone"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginRight="8dp"
        android:layout_marginTop="18dp"
        app:layout_constraintTop_toBottomOf="@+id/fullName"
        app:layout_constraintHorizontal_bias="0.187"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/location"
        android:layout_width="229dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Location"
        android:inputType="textPersonName"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/mobilePhoneNumber"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:id="@+id/confirmationText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONFIRMATION TEXT"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginRight="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <EditText
        android:id="@+id/reminder"
        android:layout_width="302dp"
        android:layout_height="46dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:hint="Reminder"
        android:inputType="textMultiLine"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="302dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Topic of Discussion"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="71dp" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:inputType="time"
        android:ems="10"
        android:id="@+id/time"
        android:hint="Time"
        tools:layout_editor_absoluteY="135dp"
        tools:layout_editor_absoluteX="276dp" />

    <EditText
        android:id="@+id/date"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Date"
        android:inputType="date"
        tools:layout_editor_absoluteY="135dp"
        tools:layout_editor_absoluteX="396dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.43"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="255dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="193dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="193dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/everyThingElse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Everything Else..."
        android:textAlignment="center"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintVertical_bias="0.905"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

</android.support.constraint.ConstraintLayout>